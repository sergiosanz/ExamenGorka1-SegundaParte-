<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  	<servlet-name>WelcomeServlet</servlet-name>
  	<servlet-class>Proyecto.servlet.WellcomeServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>listadoServlet</servlet-name>
  	<servlet-class>Proyecto.servlet.ListadoServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>selectIdServlet</servlet-name>
  	<servlet-class>Proyecto.servlet.SelectIdServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>DeleteServlet</servlet-name>
  	<servlet-class>Proyecto.servlet.DeleteServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>WellcomeServlet</servlet-name>
  	<url-pattern>wellcome</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>ListadoServlet</servlet-name>
  	<url-pattern>cargarListado</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>SelectIdServlet</servlet-name>
  	<url-pattern>selectId</url-pattern>
  </servlet-mapping>
  
   <servlet-mapping>
  	<servlet-name>DeleteServlet</servlet-name>
  	<url-pattern>delete</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
		<welcome-file>listado.jsp</welcome-file>
	</welcome-file-list>
  
</web-app>
